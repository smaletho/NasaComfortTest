@model TestNasa.Controllers.TrainingModel

@{
    ViewBag.Title = "Index";
    Layout = Url.Content("~/") + "/Views/Shared/_TrainingShell2.cshtml";
}

@section Styles{
    <style>
        
    </style>
}

@section NavigationTitle{
    @Html.Action("GetNavigationTitle", new { nav=Model.Nav_A })

}

@section Bookmarks {
    @Html.Action("RenderBookmarks")
}

@section NavigationImage{
    @Html.Action("GetNavigationImage", new { nav=Model.Nav_A })
}

@section Level1Navigation{
    @Html.Action("GetLevel1Navigation", new { nav = Model.Nav_A })
}

@section Level2Navigation{
    @Html.Action("GetLevel2Navigation", new { nav_a = Model.Nav_A, nav_b = Model.Nav_B })
}

@section Level3Navigation{
    @Html.Action("GetLevel3Navigation", new { nav_a = Model.Nav_A, nav_b = Model.Nav_B, nav_c = Model.Nav_C })
}

@section Content {
    @Html.Action("GetContent", new { nav_a = Model.Nav_A, nav_b = Model.Nav_B, nav_c = Model.Nav_C })
}